{
  "name": "emoji-emotion",
  "version": "2.1.2",
  "description": "List of emoji rated for valence in JSON",
  "license": "MIT",
  "keywords": [
    "gemoji",
    "emoji",
    "unicode",
    "word",
    "list",
    "sentiment",
    "analysis",
    "opinion",
    "mining",
    "text",
    "microblogs"
  ],
  "repository": "words/emoji-emotion",
  "bugs": "https://github.com/words/emoji-emotion/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
    "Christopher Freeley <freeley.c@husky.neu.edu>"
  ],
  "main": "index.json",
  "files": [
    "index.json"
  ],
  "dependencies": {},
  "devDependencies": {
    "alphanum-sort": "^1.0.2",
    "browserify": "^16.0.0",
    "gemoji": "^4.0.0",
    "mdast-zone": "^3.0.0",
    "plain-text-data-to-json": "^1.0.0",
    "prettier": "^1.17.1",
    "remark-cli": "^6.0.0",
    "remark-preset-wooorm": "^5.0.0",
    "tape": "^4.6.0",
    "tinyify": "^2.5.0",
    "unist-builder": "^1.0.2",
    "xo": "^0.24.0"
  },
  "scripts": {
    "generate": "node script/build-data",
    "format": "remark . -qfo && prettier --write \"**/*.js\" && xo --fix",
    "build-bundle": "browserify index.json -s emojiEmotion -o emoji-emotion.js",
    "build-mangle": "browserify index.json -s emojiEmotion -p tinyify -o emoji-emotion.min.js",
    "build": "npm run build-bundle && npm run build-mangle",
    "test-api": "node test",
    "test": "npm run format && npm run build && npm run test-api"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "ignores": [
      "emoji-emotion.js"
    ]
  },
  "remarkConfig": {
    "settings": {
      "paddedTable": false
    },
    "plugins": [
      "preset-wooorm",
      "./script/build-support"
    ]
  }
}
