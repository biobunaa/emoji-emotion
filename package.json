{
  "name": "emoji-emotion",
  "version": "3.0.0",
  "description": "List of emoji rated for valence",
  "license": "MIT",
  "keywords": [
    "gemoji",
    "emoji",
    "unicode",
    "word",
    "list",
    "sentiment",
    "analysis",
    "opinion",
    "mining",
    "text",
    "microblogs"
  ],
  "repository": "words/emoji-emotion",
  "bugs": "https://github.com/words/emoji-emotion/issues",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/wooorm"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
    "Christopher Freeley <freeley.c@husky.neu.edu>"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "devDependencies": {
    "@types/mdast": "^3.0.0",
    "@types/unist": "^2.0.0",
    "alphanum-sort": "^1.0.0",
    "c8": "^7.0.0",
    "gemoji": "^7.0.0",
    "mdast-zone": "^5.0.0",
    "plain-text-data-to-json": "^2.0.0",
    "prettier": "^2.0.0",
    "remark-cli": "^11.0.0",
    "remark-preset-wooorm": "^9.0.0",
    "type-coverage": "^2.0.0",
    "typescript": "^4.0.0",
    "unist-builder": "^3.0.0",
    "xo": "^0.52.0"
  },
  "scripts": {
    "prepack": "npm run build && npm run format",
    "generate": "node script/build-data.js",
    "build": "tsc --build --clean && tsc --build && type-coverage",
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node --conditions development test.js",
    "test-coverage": "c8 --check-coverage --100 --reporter lcov npm run test-api",
    "test": "npm run build && npm run format && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm",
      [
        "gfm",
        {
          "tablePipeAlign": false
        }
      ],
      "./script/build-support.js"
    ]
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "strict": true
  }
}
