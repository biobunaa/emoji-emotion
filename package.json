{
  "name": "emoji-emotion",
  "version": "2.1.3",
  "description": "List of emoji rated for valence in JSON",
  "license": "MIT",
  "keywords": [
    "gemoji",
    "emoji",
    "unicode",
    "word",
    "list",
    "sentiment",
    "analysis",
    "opinion",
    "mining",
    "text",
    "microblogs"
  ],
  "repository": "words/emoji-emotion",
  "bugs": "https://github.com/words/emoji-emotion/issues",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/wooorm"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
    "Christopher Freeley <freeley.c@husky.neu.edu>"
  ],
  "main": "index.json",
  "files": [
    "index.json"
  ],
  "devDependencies": {
    "alphanum-sort": "^1.0.0",
    "browserify": "^17.0.0",
    "gemoji": "^6.0.0",
    "mdast-zone": "^4.0.0",
    "plain-text-data-to-json": "^1.0.0",
    "prettier": "^2.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "tape": "^5.0.0",
    "tinyify": "^3.0.0",
    "unist-builder": "^2.0.0",
    "xo": "^0.38.0"
  },
  "scripts": {
    "generate": "node script/build-data",
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "build-bundle": "browserify index.json -s emojiEmotion -o emoji-emotion.js",
    "build-mangle": "browserify index.json -s emojiEmotion -p tinyify -o emoji-emotion.min.js",
    "build": "npm run build-bundle && npm run build-mangle",
    "test-api": "node test",
    "test": "npm run format && npm run build && npm run test-api"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "unicorn/no-fn-reference-in-iterator": "off",
      "unicorn/prefer-number-properties": "off",
      "unicorn/prefer-set-has": "off"
    },
    "ignores": [
      "emoji-emotion.js"
    ]
  },
  "remarkConfig": {
    "settings": {
      "tablePipeAlign": false
    },
    "plugins": [
      "preset-wooorm",
      "./script/build-support"
    ]
  }
}
